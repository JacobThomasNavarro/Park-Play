@model Park_Play.Models.MembersViewModel

@{
    ViewBag.Title = "PlayEventMembers";
}

<h2>Play Event Members</h2>

<br />
<table class="table">
        <tr>
            @for (int i = 0; i < Model.UsersList.Count; i++)
            {
                <td>
                    @Html.DisplayFor(model => model.UsersList[i].firstName)
                </td>
                <td>
                    @Html.DisplayFor(model => model.UsersList[i].lastName)
                </td>
            }
        </tr>
    </table>
<td>
    @Html.ActionLink("Chat with Players", "Chat", "Home") |
</td>
@Html.ActionLink("Back to Parks", "Index", "Parks")